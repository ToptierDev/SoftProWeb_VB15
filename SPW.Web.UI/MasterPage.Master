            console.log('callLoadHide')
            var hddCheckSideBar = document.getElementById('hddCheckSideBar');
            if (hddCheckSideBar.value == "checkClose") {
                $('body').toggleClass('closed-sidebar');
                $('#arrowset').toggleClass('icon-angle-right').toggleClass('icon-angle-left');
            }
        }
