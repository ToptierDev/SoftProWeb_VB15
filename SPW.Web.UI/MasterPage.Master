        function showOverlay() {
            $("#overlay").modal();
            return true;
        }
        function changeLanguage(lg) {
            showOverlay();
            if (lg == 'th-TH') {
                __doPostBack('<%= imgLGTH.UniqueID%>');
            } else {
                __doPostBack('<%= imgLGEN.UniqueID%>');
            }

        }

        function scrollAndFocus(el) {
            if ($(el).prop('tagName') == 'SELECT') {
                window.scroll(0, $(el).next().offset().top - 120);
            }
            else {
                $(el).focus();
                window.scroll(0, $(el).offset().top - 120);
            }

        }

    </script>
