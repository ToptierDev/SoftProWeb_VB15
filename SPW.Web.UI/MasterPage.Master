    <script>
        $(function () {
            getClosedSidebar();
        
            $(document).ajaxComplete(function (event, request, settings) {
                //your code here to run when ajax is complete
             
                console.log('ajaxComplete');
            });
            $('form').on('keyup keypress', function (e) {
                var keyCode = e.keyCode || e.which;
                if (keyCode === 13) {
                    var key = e.charCode ? e.charCode : e.keyCode ? e.keyCode : 0;
                    if (key == 13) {
                        e.preventDefault();
                    }
                }
            });
            attachPreventKeypress();
        });
        
        function attachPreventKeypress() {
            $('input:not(.searchfilterbox)').on("keypress", function (e) {
                var keyCode = e.keyCode || e.which;
                if (keyCode === 13) {
                    var key = e.charCode ? e.charCode : e.keyCode ? e.keyCode : 0;
                    if (key == 13) {
                        e.preventDefault();
                        var inputs = $(this).closest('form').find(':input:visible');
                        var nextele = inputs.eq(inputs.index(this) + 1);
                        if (nextele.parent().hasClass('chosen-search')) {
                            nextele = inputs.eq(inputs.index(this) + 2);
                        }
                        nextele.focus();
                    }
                }
            });
        }

        var closedSidebar;
        function setClosedSidebar(isClose) {
            if (typeof (Storage) !== "undefined") {
                localStorage.setItem("closedSidebar", isClose);
            }
            setTimeout(function () {
               // var width = $('#newGrid_wrapper').width() + 1;
                var width = $('.dataTables_scroll').width() ;
                
                console.log(width);
                $('.dataTables_scrollHeadInner').width(width);
                $('.dataTables_scrollHeadInner table').width(width);
                $('#newGrid').width(width);

            },1000)
            
        }
        function getClosedSidebar() {
            if (typeof (Storage) !== "undefined") {
                closedSidebar = localStorage.getItem("closedSidebar");
                console.log(closedSidebar);
                if (closedSidebar == 'true') {
                    $('body').addClass('closed-sidebar');
                } else {
                    console.log('removing');
                    $('body').removeClass('closed-sidebar');
                }
                console.log($('body').hasClass('closed-sidebar'));
            }
        }
    </script>
