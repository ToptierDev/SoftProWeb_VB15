    <style>
        /* Loading Spinner */
        .spinner {
            margin: 0;
            width: 70px;
            height: 18px;
            margin: -35px 0 0 -9px;
            position: absolute;
            top: 50%;
            left: 50%;
            text-align: center;
        }

            .spinner > div {
                width: 18px;
                height: 18px;
                background-color: #333;
                border-radius: 100%;
                display: inline-block;
                -webkit-animation: bouncedelay 1.4s infinite ease-in-out;
                animation: bouncedelay 1.4s infinite ease-in-out;
                -webkit-animation-fill-mode: both;
                animation-fill-mode: both;
            }

            .spinner .bounce1 {
                -webkit-animation-delay: -.32s;
                animation-delay: -.32s;
            }

            .spinner .bounce2 {
                -webkit-animation-delay: -.16s;
                animation-delay: -.16s;
            }

        @-webkit-keyframes bouncedelay {
            0%,80%,100% {
                -webkit-transform: scale(0.0);
            }

            40% {
                -webkit-transform: scale(1.0);
            }
        }

        @keyframes bouncedelay {
            0%,80%,100% {
                transform: scale(0.0);
                -webkit-transform: scale(0.0);
            }

            40% {
                transform: scale(1.0);
                -webkit-transform: scale(1.0);
            }
        }



        /*leave icon on side bar*/


        /* Closed sidebar */
        @media only screen and (min-width: 56.25em) {

            .closed-sidebar.show-sidebareicon #page-sidebar {
                transform: translateX(-200px);
                -ms-transform: translateX(-200px);
                -o-transform: translateX(-200px);
                -webkit-transform: translateX(-200px);
                visibility: visible;
            }

            .closed-sidebar.show-sidebareicon #page-content {
                margin-left: 60px;
            }

            .closed-sidebar.show-sidebareicon #header-logo {
                width: 200px;
            }

                .closed-sidebar.show-sidebareicon #header-logo .logo-content-small {
                    width: 70%;
                    height: 90%;
                    margin-left: 0;
                    left: 30px;
                    display: block;
                }

                .closed-sidebar.show-sidebareicon #header-logo .logo-content-big {
                    width: 0;
                    display: none;
                }

            .closed-sidebar.show-sidebareicon.fixed-sidebar #page-header {
                padding-left: 0;
            }

            .closed-sidebar.show-sidebareicon .scroll-sidebar {
                float: right;
                width: 60px;
                margin-top: 4px;
            }

                .closed-sidebar.show-sidebareicon .scroll-sidebar span {
                    display: none;
                }

            .closed-sidebar.show-sidebareicon #page-sidebar li a.sf-with-ul:after {
                display: none;
            }

            .closed-sidebar.show-sidebareicon #sidebar-menu {
                padding: 0;
                padding-left: 8px;
            }
        }

        @media only screen and (max-width: 1050px) {
            #header-nav-left {
                display: none;
            }
            /*#header-nav-left {
                float: left;
                width: 37%;
                height: 78px;
            }*/
        }

        @media only screen and (max-width: 319px) {
            #header-nav-left,
            #header-logo,
            #header-nav-right {
                display: none;
            }
        }


        /*@media (max-device-width: 480px) and (orientation: landscape) {
        }*/
        #page-header .user-account-btn {
            float: none !important;
        }

        .after-asterisk:after {
            content: '*';
            color: red;
        }
    </style>
